<html><head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <!--<script type="text/javascript" src="flowplayer-3.0.5.min.js"></script>-->
    <script src="../flowplayer-3.2.6.min.js"></script>


	<!-- page title -->
	<title>Minimal Flowplayer setup</title>

</head><body>

	<div id="page">
        <a
                     href="http://pseudo01.hddn.com/vod/demo.flowplayervod/Extremists.flv"
                     style="display:block;width:440px;height:247px"
                     id="player">
                </a>
<script>
    flowplayer("player", "../build/flowplayer.swf",
    {
        log: { level: 'debug', filter: 'org.flowplayer.pseudostreaming.*' },

        onBegin: function() {
            console.log("onBegin");
        },

        onStart: function() {
            console.log("onStart");
        },

        onFinish: function() {
            console.log("onFinish");
        },

        onPause: function() {
            console.log("onPause");
        },

        onResume: function() {
            console.log("onResume");
        },

        clip: {
            provider: 'pseudo'
        },

        playlist: [
//            '../build/play.png',
            { url: "http://pseudo01.hddn.com/vod/demo.flowplayervod/bbb-400.mp4" }
        ],

        plugins: {
            controls: {
                tooltips: {
                    buttons: true
                },
//                ,
//                bufferColor: 0,
                sliderColor: "#ff0000"
            }
            ,
            pseudo: {
                url: 'flowplayer.pseudostreaming.swf',
                queryString: '/foobar/${start}'
            }
        }

    });
</script>
	</div>
	
	
</body></html>